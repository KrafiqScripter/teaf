local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("teaf", "DarkTheme")
local Tab = Window:NewTab("takim")
local Section = Tab:NewSection("takim degistir")
Section:NewButton("bahriye", "bahriye ol", function()
    game.Workspace.Remotes.Bahriye:FireServer()
end)
Section:NewButton("Harekat", "harekat ol", function()
    game.Workspace.Remotes.Harekat:FireServer()
end)
Section:NewButton("Isyanci", "Isyanci ol", function()
    game.Workspace.Remotes.Isyanci:FireServer()
end)
Section:NewButton("asiz", "asiz ol", function()
    game.Workspace.Remotes.asiz:FireServer()
end)
Section:NewButton("general", "general ol", function()
    game.Workspace.Remotes.general:FireServer()
end)
Section:NewButton("sivil", "sivil ol", function()
    game.Workspace.Remotes.siviller:FireServer()
end)
Section:NewButton("tayyare", "tayyare ol", function()
    game.Workspace.Remotes.tayyare:FireServer()
end)
Section:NewButton("tsk", "tsk ol", function()
    game.Workspace.Remotes.tsk:FireServer()
end)
Section:NewButton("zaptiye", "zaptiye ol", function()
    game.Workspace.Remotes.zaptiye:FireServer()
end)
